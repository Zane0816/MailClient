  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": [
      "es5",
      "dom",
      "es2015.promise"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}